{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "^/api/.*",
      "dest": "/index.js"
    },
    {
      "src": "^/trpc/.*",
      "dest": "/index.js"
    },
    {
      "src": "^(?!.*\\.).*$",
      "dest": "/public/index.html"
    },
    {
      "src": "^/(.*)$",
      "dest": "/public/$1"
    }
  ]
}

